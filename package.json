{
  "name": "vide-plugin-prompt-js",
  "version": "0.0.1",
  "author": "vic.wang",
  "scripts": {
    "start": "babel ./src/ -d ./lib/"
  },
  "vide": {
    "type": "prompt",
    "promptExtension": [
      "js",
      "jsx"
    ],
    "promptPriority": 1
  },
  "keywords": [
    "vide-plugin",
    "prompt-js"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:debuggap/vide-plugin-prompt-js.git"
  },
  "main": "lib/index.js",
  "dependencies": {
    "vide-plugin-context-css": "*",
    "vide-plugin-context-js": "*",
    "vide-plugin-context-html": "*",
    "babel-traverse": "^6.24.1",
    "babylon": "^6.17.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-preset-es2015": "^6.18.0",
    "m-logger": "^1.0.2"
  }
}
